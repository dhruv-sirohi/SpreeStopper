{
    "name": "obj ext",
    "description": "my_ext",
    "version": "0.01",
    "manifest_version": 2,
    "icons":{
        "16": "./16x16.png",
        "32": "./32x32.png",
        "48": "./48x48.png",
        "128": "./128x128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },

    "permissions": [
        "tabs",
        "activeTab",
        "declarativeContent",
        "https://www.google.com/*",
        "https://www.amazon.ca/*"
    ],
    "content_scripts": [
        {
          "matches": [
            "https://www.amazon.com/*",
            "https://www.amazon.ca/*"
          ],
          "js": ["sitescraping.js"]
        }
      ],
      "browser_action": {
        "default_popup": "popup.html"
    }
}